<h1>Log in</h1>
<%= form_for(:session,  url: login_path) do |f| %>
	<p>
	<%= f.label :email %>
	<%= f.email_field :email%>
	</p>

	<p>
	<%= f.label :password %>
	<%=f.password_field :password %>
	</p>

	<p>
	<%= f.submit  %>
	</p>
<%end%>
	 <p><%= link_to "Cancel request and return to articles listing", events_path %></p>

	<p><%= link_to 'Log out', logout_path, method: :delete %></p>


		

		


